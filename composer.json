{
    "name":         "herzult/php-ssh",
    "type":         "library",
    "description":  "Provides an object-oriented wrapper for the php ssh2 extension.",
    "keywords":     ["ssh", "ssh2"],
    "license":      "MIT",
    "authors": [
        {
            "name": "Antoine HÃ©rault",
            "homepage": "https://github.com/Herzult"
        },
        {
            "name":     "The contributors",
            "homepage": "http://github.com/Herzult/php-ssh/contributors"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-ssh2": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/filesystem": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Ssh\\": "src/Ssh/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SshTest\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev",
            "dev-develop": "2.0.x-dev"
        }
    }
}
